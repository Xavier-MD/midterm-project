<body>
  <header>
    <%- include('./partials/header'); %>
  </header>
<main>
  <!-- Nike -->
  <!--Carousel Section -->
  <section class="carousel-section carousel-fade carousel-nike">
    <div id="carouselExampleCaptions" class="carousel slide" data-bs-ride="carousel">
      <div class="carousel-indicators">
        <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
        <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1" aria-label="Slide 2"></button>
        <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="2" aria-label="Slide 3"></button>
      </div>
      <div class="carousel-inner rounded shadow-lg p-3 mb-5 bg-body rounded">
        <div class="carousel-item active">
          <img src="/images/supreme-image.jpg" class="d-block w-100" alt="offwhite">
          <div class="carousel-caption d-none d-md-block">
            <h5>SHOP SUPREME</h5><i class="fas fa-angle-double-down"></i>

          </div>
        </div>
        <div class="carousel-item ">
          <img src="/images/gucci-image.jpg" class="d-block w-100" alt="...">
          <div class="carousel-caption d-none d-md-block">
            <h5>SHOP GUCCI</h5> <i class="fas fa-angle-double-down"></i>

          </div>
        </div>
        <div class="carousel-item ">
          <img src="/images/nike-image.jpg" class="d-block w-100" alt="...">
          <div class="carousel-caption d-none d-md-block">
            <h5>SHOP NIKE</h5> <i class="fas fa-angle-double-down"></i>

          </div>
        </div>
      </div>
      <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
      </button>
      <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
      </button>
    </div>
  </section>
  <!-- Create New Item Modal -->

  <section class="create-listing-form" >
    <h1>CREATE A NEW LISTING</h1>
    <form id="create-new-listing">
      <label>Name</label>
      <input type="text" name="name">
      <label>Description</label>
      <input type="text" name="description">
      <label>Price</label>
      <input type="text" name="price">
      <label>Seller ID</label>
      <input type="text" name="seller_id">
      <label>Product Photo (url)</label>
      <input type="text" name="product_photo_url">
      <label>Collection</label>
      <input type="text" name="collection_name">
      <button type="submit" class="btn btn-light">
        Submit
      </button>
    </form>
  </section>
  <!-- Nike Item Listings Section -->
  <section class="item-listings" id="nike">

    <div class="collection-details">
      <span class="collection-title">Shop Nike</span>
      <img class="collection-image" src="https://cdn.wallpapersafari.com/98/5/78E3Lg.jpg">
      <div class="collection-buttons">
        <div class="button-l">
          <button type="button" class="button-scroller scroll-left"><i class="fas fa-chevron-left"></i></button>
        </div>
        <div class="button-r">
          <button type="button" class="button-scroller scroll-right"><i class="fas fa-chevron-right"></i></button>
        </div>
      </div>
    </div>
    <div class="scrolling-box"></div>
  </section>

